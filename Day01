//Day01
#include <iostream>
using namespace std;

int main()
{
   int t;
   cout<<"Enter Number Of Test cases: ";
   cin >>t;
   while(t--)
   {
   int x;
   cout<<"How Much Water Did You Consumed:  ";
   cin >> x;
   if(x>=2000) //x=2000)
      cout<<"Yes";
   else
      cout<<"No";
      cout<<endl;
   }
   return 0;
}
